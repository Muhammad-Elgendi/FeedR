<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox id="AnchorPane" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.121">
    <padding> <javafx.geometry.Insets bottom="10" left="10" right="10" top="15" /> </padding>
    <children>
        <!-- -->
        <HBox alignment="CENTER">
            <JFXTextField labelFloat="true" promptText="new feed" />
            <JFXButton fx:id="searchbtn" alignment="center" graphicTextGap="10.0" onAction="#viewToEdit" styleClass="searchbtn" text="" textAlignment="CENTER">
                <graphic>
                    <FontAwesomeIconView fill="black" glyphName="SEARCH" size="30.0" strokeType="OUTSIDE" textAlignment="CENTER" />
                </graphic>
            </JFXButton>
        </HBox>
        <!-- -->
        <HBox alignment="CENTER">
            <JFXButton text="add" />
            <JFXButton text="update" />
            <JFXButton text="delete" />
        </HBox>
        <!-- -->
        <VBox>
            <HBox alignment="CENTER_RIGHT">
                <JFXButton text="Clear All" />
            </HBox>
            <TableView>
                <columns>
                    <TableColumn text="id">

                    </TableColumn>
                    <TableColumn text="title">

                    </TableColumn>
                    <TableColumn minWidth="150" text="describtion">

                    </TableColumn>
                    <TableColumn text="date">

                    </TableColumn>
                </columns>
            </TableView>
        </VBox>
        <!-- -->
    </children>
    <stylesheets>
        <URL value="@allpageview.css" />
    </stylesheets>
</VBox>
